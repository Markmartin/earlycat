.container{
}
.head{
  display: flex;
  margin: 0 26rpx;
  align-items: center;
  background: #F7F7F7;
  border-radius: 32rpx;
  height: 68rpx;
  padding: 0 26rpx;
  margin-bottom: 24rpx;
}
.head image{
  width: 28rpx;
  min-width: 28rpx;
  height: 28rpx;
}
.head input{
  margin-left: 20rpx;
  flex: 1;
  height: 68rpx;
  line-height: 68rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #333;
}
.searchCover{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 70rpx;
  z-index: 100;
}
.issue{
  position: fixed;
  right: 30rpx;
  bottom: 200rpx;
  z-index: 10;
  background-color: #04B64E;
  border-radius: 100%;
  width: 110rpx;
  height: 110rpx;
}
.issue image{
  width: 100%;
  height: 100%;
}

.list{
  padding: 0 30rpx;
  margin-top: 40rpx;
}
.list .item{
  overflow: hidden;
  display: flex;
  margin-bottom: 50rpx;
}
.list .item .pic{
  min-width: 220rpx;
  width: 220rpx;
  height: 152rpx;
  border-radius: 12rpx;
  margin-right: 20rpx;
}
.list .item .cont{
  width: 450rpx;
  overflow: hidden;
}
.list .item .tt{
  font-size:30rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(51,51,51,1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 10rpx;
}
.list .item .tag{
  min-height: 64rpx;
  margin-right: -12rpx;
}
.list .item .tag text{
  background:rgba(234,255,243,1);
  border-radius:6rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
  border:1px solid rgba(4,182,78,1);
  display: inline-block;
  height: 30rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
  font-size:20rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(4,182,78,1);
}
.list .item .address{
  font-size:24rpx;
  font-family:PingFangSC-Regular,PingFang SC;
  font-weight:400;
  color:rgba(102,102,102,1);
}
.list .item .address text{
  display: block;
  width: 260rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.list .item .site{
  min-width: 30rpx;
  width: 30rpx;
  height: 30rpx;
  margin-right: 8rpx;
}



