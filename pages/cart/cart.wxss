page {
  height: 100%;
  min-height: 100%;
  background: #f4f4f4;
}

.container {
  background: #f4f4f4;
  width: 100%;
  height: auto;
  min-height: 100%;
  overflow: hidden;
}

.service-policy {
  width: 750rpx;
  height: 73rpx;
  background: #f4f4f4;
  padding: 0 31.25rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: space-between;
}

.service-policy .item {
  background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/servicePolicyRed-518d32d74b.png) 0 center no-repeat;
  background-size: 10rpx;
  padding-left: 15rpx;
  display: flex;
  align-items: center;
  font-size: 25rpx;
  color: #666;
}

.no-login {
  width: 100%;
  height: auto;
  margin: 0 auto;
}

.no-login .c {
  width: 100%;
  height: auto;
  margin-top: 200rpx;
}

.no-login .c image {
  margin: 0 auto;
  display: block;
  text-align: center;
}

.no-login .c text {
  margin: 0 auto;
  display: block;
  width: 258rpx;
  height: 59rpx;
  line-height: 29rpx;
  text-align: center;
  font-size: 35rpx;
  color: #999;
}

.no-login button {
  width: 90%;
  margin: 0 auto;
  color: #fff;
  font-size: 30rpx;
  height: 96rpx;
  line-height: 96rpx;
  right: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  position: flex;
  bottom: 0;
  left: 0;
  border-radius: 0;
  padding: 0;
  margin-left: 5%;
  text-align: center;
  /* padding-left: -5rpx; */
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  border-top-right-radius: 50rpx;
  border-bottom-right-radius: 50rpx;
  letter-spacing: 3rpx;
  background-image: linear-gradient(to right, #9a9ba1 0%, #9a9ba1 100%);
}

.no-cart {
  width: 100%;
  height: auto;
  margin: 0 auto;
}

.no-cart .c {
  width: 100%;
  height: auto;
  margin-top: 200rpx;
}

.no-cart .c image {
  margin: 0 auto;
  display: block;
  text-align: center;
}

.no-cart .c text {
  margin: 0 auto;
  display: block;
  width: 258rpx;
  height: 29rpx;
  line-height: 29rpx;
  text-align: center;
  font-size: 29rpx;
  color: #999;
}

.cart-view {
  width: 100%;
  height: auto;
  overflow: hidden;
}

.cart-view .list {
  height: auto;
  width: 100%;
  position: relative;
  z-index: 1;
  overflow: hidden;
  margin-bottom: 120rpx;
}

.cart-view .group-item {
  height: auto;
  width: 100%;
  background: #fff;
  margin-bottom: 18rpx;
}

.cart-view .item {
  height: 210rpx;
  width: 100%;
  position: relative;
  /* overflow: hidden; */
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f4f4f4;
}

.cart-view .item .checkbox {
  /* float: left; */
  height: 210rpx;
  width: 80rpx;
  /* margin: 65rpx 18rpx 65rpx 26rpx; */
  background: url(https://earlycat-app.oss-cn-shanghai.aliyuncs.com/unselected.png) no-repeat center;
  background-size: 34rpx 34rpx;
}

.cart-view .item .checkbox.checked {
  background: url(https://earlycat-app.oss-cn-shanghai.aliyuncs.com/selected.png) no-repeat center;
  background-size: 34rpx 34rpx;
}

.cart-view .item .checkbox.disabled {
  /* background: #ccc; */
  background: url(https://earlycat-app.oss-cn-shanghai.aliyuncs.com/disable_select.png) no-repeat center;
  background-size: 34rpx 34rpx;
}

.disabledTitle {
  text-align: center;
  padding: 20rpx;
  font-size: 28rpx;
}

.disabledTitle text {
  border: #ebebeb 1rpx solid;
  padding: 10rpx 30rpx;
  border-radius: 40rpx;
}

.cart-view .item .cart-goods {
  /* float: left; */
  height: 210rpx;
  width: 670rpx;
  display: flex;
  align-items: center;
}

.cart-view .item .cart-goods .goods-combination-image {
  position: relative;
  height: 160rpx;
  width: 160rpx;
}

.cart-view .item .cart-goods .goods-combination-image .pre-icon {
  position: absolute;
  left: 0;
  /* top: 20rpx; */
  width: 60rpx;
  height: 60rpx;
}

.cart-view .item .img {
  height: 160rpx;
  width: 160rpx;
  background: #f4f4f4;
}

.cart-view .item .info {
  height: 210rpx;
  width: 510rpx;
  padding: 20rpx;
  box-sizing: border-box;
}

.cart-view .item .t {
  width: 100%;
  height: 80rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.cart-view .item .name {
  width: 420rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #333;
}

.cart-view .item .num {
  line-height: 40rpx;
  font-size: 30rpx;
  width: 40rpx;
  text-align: right;
  font-weight: bold;
}

.cart-view .item .attr {
  /* margin-bottom: 17rpx; */
  height: 40rpx;
  line-height: 40rpx;
  font-size: 25rpx;
  color: #666;
  /* overflow: hidden; */
}

.cart-view .item .b {
  height: 28rpx;
  line-height: 28rpx;
  font-size: 25rpx;
  color: #333;
  overflow: hidden;
}

.cart-view .item .price {
  float: left;
}

.cart-view .item .open {
  height: 28rpx;
  width: 150rpx;
  display: block;
  float: right;
  background: url(http://nos.netease.com/mailpub/hxm/yanxuan-wap/p/20150730/style/img/icon-normal/arrowDown-d48093db25.png) right center no-repeat;
  background-size: 25rpx;
  font-size: 25rpx;
  color: #333;
}

.cart-view .item.edit .t {
  /* display: none; */
  margin-bottom: -10rpx;
}

.cart-view .item.edit .attr {
  text-align: right;
  background: url(http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/arrow-right1-e9828c5b35.png) right center no-repeat;
  padding-right: 25rpx;
  background-size: 12rpx 20rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 25rpx;
  color: #999;
  overflow: hidden;
}

.cart-view .item.edit .b {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 10rpx;
  height: 55rpx;
  overflow: hidden;
}

.cart-view .item.edit .price {
  line-height: 52rpx;
  height: 52rpx;
  font-size: 30rpx;
  /* flex: 1; */
  color: #80ba64;
}

.cart-view .item .selnum {
  display: none;
}

.cart-view .item.edit .selnum {
  width: 235rpx;
  height: 52rpx;
  border: 1rpx solid #ccc;
  display: flex;
}

.selnum .cut {
  width: 70rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}

.selnum .number {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 68.75rpx;
  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;
  float: left;
}

.selnum .add {
  width: 80rpx;
  height: 100%;
  text-align: center;
  line-height: 50rpx;
}

.cart-view .group-item .header {
  width: 100%;
  height: 94rpx;
  line-height: 94rpx;
  padding: 0 26rpx;
  border-bottom: 1px solid #f4f4f4;
}

.cart-view .promotion .icon {
  display: inline-block;
  height: 24rpx;
  width: 15rpx;
}

.cart-view .promotion {
  margin-top: 25.5rpx;
  float: left;
  height: 43rpx;
  width: 480rpx;
  /*margin-right: 84rpx;*/
  line-height: 43rpx;
  font-size: 0;
}

.cart-view .promotion .tag {
  border: 1px solid #f48f18;
  height: 37rpx;
  line-height: 31rpx;
  padding: 0 9rpx;
  margin-right: 10rpx;
  color: #f48f18;
  font-size: 24.5rpx;
}

.cart-view .promotion .txt {
  height: 43rpx;
  line-height: 43rpx;
  padding-right: 10rpx;
  color: #333;
  font-size: 29rpx;
  overflow: hidden;
}

.cart-view .get {
  margin-top: 18rpx;
  float: right;
  height: 58rpx;
  padding-left: 14rpx;
  border-left: 1px solid #d9d9d9;
  line-height: 58rpx;
  font-size: 29rpx;
  color: #333;
}

.give-away {
  position: fixed;
  left: 0;
  bottom: 100rpx;
  width: 100%;
  height: 60rpx;
  background-color: white;
  border-top: 1rpx solid rgba(0, 0, 0, 0.2);
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.2);
  display: flex;
  justify-content: space-between;
  z-index: 999;
}

.give-away .label {
  color: #80ba64;
  line-height: 60rpx;
  padding-left: 30rpx;
}

.give-away .options {
  display: flex;
  height: 60rpx;
  justify-content: space-around;
  align-items: center;
}

.give-away .options .selected {
  color: white;
  background-color: #80ba64;
  border: 1px solid transparent;
}

.give-away .options .unselected {
  color: #80ba64;
  background-color: white;
  border: 1px solid #80ba64;
}

.give-away .options .option {
  display: inline-block;
  /* padding: 5rpx 10rxp; */
  border-radius: 20rpx;
  width: 120rpx;
  height: 40rpx;
  line-height: 40rpx;
  /* color: #80ba64; */
  text-align: center;
  margin-right: 20rpx;
}


.cart-bottom {
  position: fixed;
  z-index: 100;
  bottom: 0;
  left: 0;
