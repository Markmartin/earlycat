.container{
  padding-bottom: 240rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}
.content {
  width: 100%;
  height: auto;
  font-size: 0;
}

.content image {
  display: inline-block;
  width: 100%;
}

.comments {
  width: 100%;
  height: auto;
  padding-left: 30rpx;
  background: #fff;
  margin-top: 20rpx;
}

.comments .h {
  height: 93rpx;
  line-height: 93rpx;
  width: 720rpx;
  padding-right: 30rpx;
  border-bottom: 1px solid #d9d9d9;
}

.comments .h .t {
  display: block;
  float: left;
  width: 50%;
  font-size: 29rpx;
  color: #333;
}

.comments .h .i {
  display: block;
  float: right;
  margin-top: 30rpx;
  width: 33rpx;
  height: 33rpx;
}

.comments .b {
  height: auto;
  width: 720rpx;
}

.comments .item {
  height: auto;
  width: 720rpx;
  overflow: hidden;
  border-bottom: 1px solid #d9d9d9;
}

.comments .info {
  height: 127rpx;
  width: 100%;
  padding: 33rpx 0 27rpx 0;
}

.comments .user {
  float: left;
  width: auto;
  height: 67rpx;
  line-height: 67rpx;
  font-size: 0;
}

.comments .user .avatar {
  display: block;
  float: left;
  width: 67rpx;
  height: 67rpx;
  margin-right: 17rpx;
  border-radius: 50%;
}

.comments .user .nickname {
  display: block;
  width: auto;
  float: left;
  height: 66rpx;
  overflow: hidden;
  font-size: 29rpx;
  line-height: 66rpx;
}

.comments .time {
  display: block;
  float: right;
  width: auto;
  height: 67rpx;
  line-height: 67rpx;
  color: #7f7f7f;
  font-size: 25rpx;
  margin-right: 30rpx;
}

.comments .comment {
  width: 720rpx;
  padding-right: 30rpx;
  line-height: 45.8rpx;
  margin-bottom: 30rpx;
  font-size: 29rpx;
  color: #333;
}
.comments .imgs {
  width: 720rpx;
  height: 150rpx;
  margin-bottom: 25rpx;
}

.comments .imgs .img {
  height: 150rpx;
  width: 150rpx;
  margin-right: 28rpx;
}

.comments .load {
  width: 720rpx;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 38.5rpx;
}

.no-comments {
  height: 297rpx;
}

.no-comments .txt {
  height: 43rpx;
  line-height: 43rpx;
  display: block;
  width: 100%;
  text-align: center;
  font-size: 29rpx;
  color: #7f7f7f;
}

.no-comments .icon {
  margin: 48rpx auto 18rpx auto;
  height: 130rpx;
  display: block;
  width: 115rpx;
}

.sv-goods {
  width: 100%;
  height: auto;
  padding-left: 30rpx;
  background: #fff;
  margin-top: 20rpx;
}

.topic-goods .h {
  height: 93rpx;
  line-height: 93rpx;
  width: 720rpx;
  padding-right: 30rpx;
}

.topic-goods .h .t {
  display: block;
  float: left;
  width: 50%;
  font-size: 29rpx;
  color: #333;
}

.topic-goods .h .i {
  display: block;
  float: right;
  margin-top: 30rpx;
  width: 33rpx;
  height: 33rpx;
}

.topic-goods .b .item {
  border-top: 1px solid #d9d9d9;
  margin: 0;
  width: 710rpx;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.topic-goods .b .img {
  margin-right: 12rpx;
  min-width: 220rpx;
  width: 220rpx;
  height: 220rpx;
}

.topic-goods .b .right {
  width: 476rpx;
}

.topic-goods .b .text {
  overflow: hidden;
  width: 476rpx;
}

.topic-goods .b .name {
  width: 330rpx;
  display: block;
  color: #333;
  line-height: 50rpx;
  font-size: 30rpx;
}

.topic-goods .b .desc {
  width: 476rpx;
  display: block;
  color: #999;
  line-height: 50rpx;
  font-size: 25rpx;
}

.topic-goods .b .price {
  width: 476rpx;
  display: flex;
  color: #b4282d;
  line-height: 50rpx;
  font-size: 33rpx;
}

.rec-box {
  width: 690rpx;
  height: auto;
  margin: 0 30rpx;
}

.rec-box .h {
  position: relative;
  width: 690rpx;
  height: 96rpx;
  /*border-bottom: 1px solid #d0d0d0;*/
  margin-bottom: 32rpx;
}

.rec-box .h .txt {
  display: inline-block;
  position: absolute;
  background: #f4f4f4;
  top: 59rpx;
  left: 200rpx;
  width: 290rpx;
  height: 45rpx;
  line-height: 45rpx;
  font-size: 30rpx;
  color: #999;
  text-align: center;
}

.rec-box .b .item {
  width: 690rpx;
  height: 397rpx;
  padding: 24rpx 24rpx 30rpx 24rpx;
  background: #fff;
  margin-bottom: 30rpx;
}

.rec-box .b .item .img {
  height: 278rpx;
  width: 642rpx;
}

.rec-box .b .item .title {
  display: block;
  margin-top: 30rpx;
  height: 30rpx;
  width: 642rpx;
  font-size: 28rpx;
}
@import "../../lib/wxParse/wxParse.wxss";


.goodsList{
  position: relative;
  padding: 40rpx 16rpx 20rpx;
  margin-right: -40rpx;
}
.goodsList .item{
  padding: 0 20rpx 20rpx 20rpx;
  margin-bottom: 24rpx;
  position: relative;
  background-color: #fff;
  display: inline-block;
  width: 350rpx;
  margin-right: 18rpx;
  border-radius: 12rpx;
  box-shadow: 0 3px 4px 0 rgba(0,0,0,0.1);
  text-align: center;
}
.goodsList .item .pic{
  width: 224rpx;
  height: 224rpx;
  display: block;
  margin: 0 auto;
}
.goodsList .item .tt{
  font-family: PingFangSC-Semibold;
  font-size: 28rpx;
  color: #333333;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goodsList .item .txt{
  display: inline-block;
  height: 46rpx;
  width: 260rpx;
  padding: 0 10rpx;
  padding-bottom: 11rpx;
  border-bottom: #E6E6E6 2rpx solid;
  text-align: center;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #AC4C43;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goodsList .item .price{
  font-family: PingFangSC-Light;
  font-size: 22rpx;
  color: #999999;
  text-decoration: line-through;
  text-align: left;
}
.goodsList .item .txt1{
  font-size: 28rpx;
  color: #FF383A;
  letter-spacing: 0.65px;
  text-align: left;
}
.goodsList .item .price1{
  font-family: PingFangSC-Semibold;
  font-size: 48rpx;
  color: #FF383A;
  text-align: left;
}
.goodsList .item .price1 text{
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #FF383A;
}
.goodsList .item .cart{
  width: 76rpx;
  height: 76rpx;
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  background-color: #FF383A;
  border-radius: 100%;
}
.goodsList .item .cart image{
  width: 76rpx;
  height: 76rpx;
}
.footer{
  position: fixed;
  padding-top: 50rpx;
  padding-bottom: 20rpx;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 100;
}
.packBtn{
  width: 720rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  margin: 0 auto;
  background-color: #FFEE86;
  font-family: PingFangSC-Medium;
  font-size: 40rpx;
  color: #9B2816;
  border-radius: 71rpx;
  margin-bottom: 32rpx;
}
.toppic_ico{
  width: 558rpx;
  height: 48rpx;
  display: block;
  margin: 0 auto;
}