}
image{
  width: 316rpx;
  height: 316rpx;
  display:block;
  margin:0 auto;
  margin-top: 36rpx;
  margin-bottom: 80rpx;
}
.dot1{
  display: inline-block;
  width: 8.8rpx;
  height: 8.8rpx;
  background-image: linear-gradient(225deg, #0ADC86 15%, #04B64E 85%);
  border-radius: 5.5px;
  vertical-align: middle;
}
.dot2{
  display: inline-block;
  width: 13.2rpx;
  height: 13.2rpx;
