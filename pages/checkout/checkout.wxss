.container {
  height: 100%;
  background: #f4f4f4;
  width: 750rpx;
  box-sizing: border-box;
  overflow-x: hidden;
}

.container .address-box {
  width: 750rpx;
  box-sizing: border-box;
  /* height: 166.55rpx; */
  background: url("http://yanxuan.nosdn.127.net/hxm/yanxuan-wap/p/20161201/style/img/icon-normal/address-bg-bd30f2bfeb.png") 0 0 repeat-x #fff;
  background-size: 62.5rpx 10.5rpx;
  margin-bottom: 20rpx;
  padding-top: 10.5rpx;
}

.container .address-item {
  height: 250rpx;
  display: flex;
  align-items: center;
  /* height: 155.55rpx; */
  background: #fff;
  /* padding: 41.6rpx 0 41.6rpx 31.25rpx; */
}

.container .address-item.address-empty {
  line-height: 75rpx;
  text-align: center;
  justify-content: space-between;
}

.container .address-box .l {
  width: 125rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  overflow: hidden;
}

.container .address-box .l .name {
  width: 125rpx;
  height: 45rpx;
  font-size: 30rpx;
  text-align: center;
  color: #333;
  /* margin-bottom: 5.2rpx; */
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.container .address-box .l .default {
  margin-top: 10rpx;
  padding: 5rpx 10rpx;
  width: 65rpx;
  line-height: 30rpx;
  font-size: 25rpx;
  text-align: center;
  color: #b4282d;
  border-radius: 20rpx;
  border: 1rpx solid #b4282d;
  visibility: visible;
}

.container .address-box .m {
  width: 565rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: center;
  padding: 0 20rpx;
}

.container .address-box .mobile {
  height: 30rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  overflow: hidden;
  color: #333;
  font-weight: bold;
}

.container .address-box .address {
  margin-top: 10rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  /* overflow: hidden; */
  color: #444;
}

.container .address-box .r {
  width: 60rpx;
  height: 100%;
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .address-box .r image {
  display: block;
  width: 52rpx;
  height: 52rpx;
}

.container .coupon-box {
  width: 750rpx;
  box-sizing: border-box;
  height: auto;
  overflow: hidden;
  background: #fff;
}

.container .coupon-box .coupon-item {
  width: 100%;
  height: 108.3rpx;
  overflow: hidden;
  background: #fff;
  display: flex;
  padding-left: 31.25rpx;
}

.container .coupon-box .l {
  flex: 1;
  height: 43rpx;
  line-height: 43rpx;
  padding-top: 35rpx;
}

.container .coupon-box .l .name {
  float: left;
  font-size: 30rpx;
  color: #666;
}

.container .coupon-box .l .txt {
  float: right;
  font-size: 30rpx;
  color: #666;
}

.container .coupon-box .r {
  margin-top: 15.5rpx;
  margin-right: 15.5rpx;
  width: 77rpx;
  height: 77rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .coupon-box .r image {
  width: 52.078rpx;
  height: 52.078rpx;
}

.container .message-box {
  margin-top: 20rpx;
  width: 750rpx;
  box-sizing: border-box;
  height: auto;
  overflow: hidden;
  background: #fff;
}

.container .message-box .message-item {
  height: 100rpx;
  line-height: 100rpx;
  overflow: hidden;
  background: #fff;
  display: block;
  padding: 0 31.25rpx;
  font-size: 40rpx;
  font-weight: bold;
}

.container .order-box {
  margin-top: 20rpx;
  width: 750rpx;
  box-sizing: border-box;
  height: auto;
  overflow: hidden;
  background: #fff;
}

.container .order-box .order-item {
  height: 75rpx;
  overflow: hidden;
  background: #fff;
  display: flex;
  margin-left: 31.25rpx;
  padding-right: 31.25rpx;
  padding-top: 26rpx;
  border-bottom: 1px solid #d9d9d9;
}

.container .order-box .order-item .l {
  float: left;
  height: 52rpx;
  width: 50%;
  line-height: 52rpx;
  overflow: hidden;
}

.container .order-box .order-item .r {
  float: right;
  text-align: right;
  width: 50%;
  height: 52rpx;
  line-height: 52rpx;
  overflow: hidden;
}

.container .order-box .order-item.no-border {
  border-bottom: none;
}

.container .payment-method {
  box-sizing: border-box;
  position: relative;
  margin-top: 20rpx;
  width: 100%;
}

.container .payment-method .payment-label {
  padding-left: 31.25rpx;
  padding-bottom: 10rpx;
  font-size: 30rpx;
  font-weight: bold;
}

.container .payment-method .payment-cell {
  position: relative;
  padding-left: 31.25rpx;
  height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: white;
}

.container .payment-method .payment-cell .payment-cell-method {
  flex-grow: 1;
}

.container .payment-method .payment-cell .payment-cell-checkbox {
  margin-right: 31.25rpx;
  width: 40rpx;
  height: 40rpx;
}

.container .payment-method .payment-cell .payment-cell-line {
  position: absolute;
  left: 31.25rpx;
  bottom: 1rpx;
  width: 718rpx;
  height: 1rpx;
  background-color: #d9d9d9;
}

.container .order-container {
  width: 750rpx;
  box-sizing: border-box;
  padding-bottom: 130rpx;
}

.container .goods-items {
  margin-top: 20rpx;
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding-left: 31.25rpx;
  /* margin-bottom: 60rpx; */
}

.container .no-pre-sale-list {
  margin-bottom: 120rpx;
}

.container .goods-items .item {
  height: 192rpx;
  padding-right: 31.25rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid rgba(0, 0, 0, 0.15);
}

.container .goods-items .item.no-border, .container .goods-items .item:last-child {
  border-bottom: none;
}

.container .goods-items .img {
  height: 145.83rpx;
  width: 145.83rpx;
  background-color: #f4f4f4;
  margin-right: 20rpx;
  position: relative;
}

.container .goods-items .img .pre-icon {
  position: absolute;
  left: 0;
  top: 0;
  width: 60rpx;
  height: 60rpx;
}

.container .goods-items .img image {
  height: 145.83rpx;
  width: 145.83rpx;
  z-index: 10;
}

.container .goods-items .info {
  /* flex: 1; */
  width: 550rpx;
  height: 145.83rpx;
  padding-top: 5rpx;
}

.container .goods-items .t {
  height: 33rpx;
  line-height: 33rpx;
  margin-bottom: 10rpx;
  /* overflow: hidden; */
  font-size: 30rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.container .goods-items .t .name {
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  white-space: nowrap;
  /* float: left; */
}

.container .goods-items .t .number {
  display: block;
  padding-right: 20rpx;
  /* float: right; */
  text-align: right;
}

.container .goods-items .m {
  height: 29rpx;
  overflow: hidden;
  line-height: 29rpx;
  margin-bottom: 25rpx;
  font-size: 25rpx;
  color: #666;
}

.container .goods-items .b {
  height: 41rpx;
  overflow: hidden;
  line-height: 41rpx;
  font-size: 30rpx;
  color: #333;
}

.container .goods-items .shipping-fee {
  display: flex;
  justify-content: space-between;
  line-height: 100rpx;
  height: 100rpx;
}

.container .goods-items .shipping-fee .shipping-fee-value {
  margin-right: 60rpx;
}

.container .order-total {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 100rpx;
  width: 750rpx;
  box-sizing: border-box;
  display: flex;
}

.container .order-total .l {
  flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  color: #b4282d;
  background: #fff;
  font-size: 33rpx;
  padding-left: 31.25rpx;
  border-top: 1rpx solid rgba(0, 0, 0, 0.2);
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.2);
}

.container .order-total .c {
  width: 233rpx;
  height: 100rpx;
  background: #ff9314;
  border: 1px solid #ff9314;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}

.container .order-total .r {
  width: 233rpx;
  height: 100rpx;
  background: #80ba64;
  border: 1px solid #80ba64;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}

.container .noCommunity {
  text-align: center;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.6);
  height: 100rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #ffffff;
  letter-spacing: 0;
  padding: 0 26rpx;
}

.container .address-box .radio {
  display: inline-block;
  margin-right: 40rpx;
  height: 66rpx;
  line-height: 66rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #333;
}

.container .address-box .radio:last-child {
  border-bottom: 0;
}

.container .address-box radio .wx-radio-input {
  width: 30rpx;
  height: 30rpx;
  border: 2rpx solid #f5f5f5;
  border-radius: 100%;
  background: none;
  margin-right: 10rpx;
}

.container .address-box radio .wx-radio-input.wx-radio-input-checked {
  border: 2rpx solid #04b64e !important;
}

.container .address-box radio .wx-radio-input.wx-radio-input-checked::before {
  width: 60%;
  height: 60%;
  background: #04b64e;
  border-radius: 100%;
  content: '';
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
